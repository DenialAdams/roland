{
    "name": "roland",
    "publisher": "brickcodes",
    "repository": "https://www.github.com/DenialAdams/roland",
    "displayName": "Roland",
    "description": "Support for the Roland programming language",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.61.0"
    },
