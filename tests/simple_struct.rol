struct X {
   my_field: u8,
   my_other_field: Y,
}

proc main() {
   let instance = X {
      my_field: 10,
      my_other_field: Y {
         another_field: 300
      },
   };

   print_bool(instance.my_field == 10);
   print_bool(instance.my_other_field == 300);
}

struct Y {
   another_field: u64,
}
