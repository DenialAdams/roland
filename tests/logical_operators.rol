proc f_false() -> bool {
    println("f_false");
    return false;
}

proc f_true() -> bool {
    println("f_true");
    return true;
}

proc main() {
    let result = f_true() or f_false();
    println_bool(result);
    println("---");
    result = f_false() and f_true();
    println_bool(result);
}
