proc main() {
   print_bool(true | false);
   print_bool(true | true);
   print_bool(true & false);
   print_bool(true & true);
   print_bool(true ^ false);
   print_bool(true ^ true);
   let x: u8 = 123;
   print_bool(x & 0 == 0);
   print_bool(x | 0 == 123);
   print_bool(x & 255 == 123);
   print_bool(x | 255 == 255);
   print_bool(x ^ x == 0);
   print_bool(x ^ 0 == 123);
   print_bool(x ^ 255 == 132);
   let y: i8 = 2;
   print_bool(!y == -3);
   let z: u8 = 2;
   print_bool(!z == 253);
}
