struct Foo {
   x: u64
}

struct Bar {
   x1: u32,
   x2: u16,
   x3: u8,
   x4: u8,
}

proc main() {
   let foo = Foo {
      x: 0xfedcba98_8455_cd_23,
   };

   // lvalue transmute
   let bar = foo transmute Bar;

   // this test assumes little endianness...¯\_(ツ)_/¯
   print_bool(bar.x1 == 0x8455cd23);
   print_bool(bar.x2 == 0xba98);
   print_bool(bar.x3 == 0xdc);
   print_bool(bar.x4 == 0xfe);

   // rvalue transmute
   let bar2 = Foo {
      x: 0xfedcba98_8455_cd_23,
   } transmute Bar;
   print_bool(bar2.x1 == 0x8455cd23);
   print_bool(bar2.x2 == 0xba98);
   print_bool(bar2.x3 == 0xdc);
   print_bool(bar2.x4 == 0xfe);
}
