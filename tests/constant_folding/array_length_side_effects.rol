struct Foo {
   x: u8,
   y: [u8; 2],
}

proc main() {
   let a = Foo {
      x: disarmMissiles(),
      y: [1, 2]
   }.y.length;
   let b = foo([getTrue(), false].length).length;
   println(uint_to_string(a as u64));
   println(uint_to_string(b as u64));
}

proc getTrue() -> bool {
   println("true");
   return true;
}

proc foo(x: usize) -> [u8; 2] {
   println("side effect");
   return [1, 1];
}

proc disarmMissiles() -> u8 {
   println("disarmed");
   return 10;
}
__END__
run:
disarmed
true
side effect
2
2
