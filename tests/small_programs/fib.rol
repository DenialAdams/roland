proc main() {
   let x = fib(9);
   let z = fast_fib(9);
   println_bool(x == 34);
   println_bool(x == z);
}

proc fib(n: u64) -> u64 {
   if n == 0 {
      return 0;
   } else if n == 1 {
      return 1;
   }
   return fib(n - 1) + fib(n - 2);
}

proc fast_fib(n: u64) -> u64 {
   if n == 0 {
      return 0;
   }

   let twice_before: u64 = 0;
   let once_before: u64 = 1;
   let sum: u64 = 1;
   let i: u64 = 1;
   loop {
      if i == n {
         return sum;
      }
      sum = once_before + twice_before;
      twice_before = once_before;
      once_before = sum;
      i = i + 1;
   }
}
__END__
